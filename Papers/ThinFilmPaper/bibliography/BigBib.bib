	File = {:F$\backslash$:/Dropbox/Rossmanith/Literature/RKDG Method Part 5.pdf:pdf},
	Issn = {00219991},
	Journal = {J. Comput. Physics},
	Keywords = {Euler equations,discontinuous Galerkin,slope limiters},
	Number = {2},
	Pages = {199--224},
	Title = {The {R}unge--{K}utta Discontinuous {G}alerkin Method for Conservation Laws {V}},
